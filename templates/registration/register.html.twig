{% extends 'base.html.twig' %}



{% block body %}

    <div class="container">
        <h1 class="h3 mb-3 font-weight-normal">Please sign up</h1>
        {{ form_start(form) }}
            {{ form_row(form.username) }}
            {{ form_row(form.email) }}
            {{ form_row(form.password) }}

        <button type="submit">Register!</button>
        {{ form_end(form) }}
    </div>

{% endblock %}
